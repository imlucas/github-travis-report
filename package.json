{
  "name": "github-travis-report",
  "description": "module that given an organization and an attribute produces a report based on versions of the attribute in the .travis.yml file",
  "version": "0.1.0",
  "scripts": {
    "fmt": "mongodb-js-fmt",
    "check": "mongodb-js-precommit",
    "test": "mocha",
    "ci": "npm run check && npm test"
  },
  "bin": {
    "github-travis-report": "./bin/cli.js"
  },
  "homepage": "http://github.com/mongodb-js/github-travis-report",
  "repository": {
    "type": "git",
    "url": "git://github.com/mongodb-js/github-travis-report.git"
  },
  "bugs": {
    "url": "git://github.com/mongodb-js/github-travis-report.git/issues"
  },
  "dependencies": {
    "event-stream": "^3.3.3",
    "github": "^8.1.1",
    "js-yaml": "^3.6.1",
    "mongodb-extended-json": "^1.6.3",
    "mongodb-js-repo-list": "^0.1.0",
    "yamljs": "^0.2.7",
    "yargs": "^6.3.0"
  },
  "devDependencies": {
    "sinon": "^1.17.4",
    "eslint-config-mongodb-js": "^2.2.0",
    "mocha": "^3.1.2",
    "mongodb-js-fmt": "^0.0.3",
    "mongodb-js-precommit": "^0.2.8",
    "pre-commit": "^1.1.2"
  },
  "license": "Apache-2.0",
  "precommit": [
    "check"
  ]
}
